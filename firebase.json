{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {"source": "**/api/getData",
      "function": "getData"
    },
      {
        "source": "**",
        "destination": "/index.html"
      }      
    ]
  }
}